<Window x:Class="Proghebdo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proghebdo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <local:ProgJour HorizontalAlignment="Left" Height="180" Margin="29,30,0,0" VerticalAlignment="Top" Width="569" RenderTransformOrigin="0.5,0.5">
            <local:ProgJour.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.098"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.498"/>
                </TransformGroup>
            </local:ProgJour.RenderTransform>
        </local:ProgJour>
    </Grid>
</Window>
